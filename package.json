{
  "name": "npmstars",
  "version": "0.0.4",
  "description": "show stars number of your project dependencies",
  "main": "./lib/star_counter.js",
  "bin": {
    "npm-stars": "bin/npm-stars.js"
  },
  "directories": {
    "bin": "./bin",
    "test": "./test",
    "lib": "./lib"
  },
  "scripts": {
    "transpile": "[ -d src ] && ./node_modules/.bin/babel src --out-dir lib || true",
    "pretest": "npm run transpile",
    "prepublish": "npm run transpile",
    "test": "mocha --ui bdd --compilers js:babel-core/register --require babel-polyfill --recursive --reporter spec --check-leaks -t 20000 test",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- --ui bdd --compilers js:babel-core/register --require babel-polyfill --recursive --reporter spec --check-leaks -t 20000 test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.2.0",
    "mocha": "^3.0.2",
    "istanbul": "^1.0.0-alpha.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^3.5.5",
    "cheerio": "^0.22.0",
    "isomorphic-fetch": "^2.2.1",
    "request": "^2.74.0",
    "babel-polyfill": "^6.13.0"
  }
}
